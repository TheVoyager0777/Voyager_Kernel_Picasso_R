static const struct nvkm_engine_func
gv100_ce = {
	.intr = gp100_ce_intr,
	.sclass = {
		{ -1, -1, VOLTA_DMA_COPY_A },
		{}
	}
};

int
gv100_ce_new(struct nvkm_device *device, int index,
	     struct nvkm_engine **pengine)
{
	return nvkm_engine_new_(&gv100_ce, device, index, true, pengine);
}
